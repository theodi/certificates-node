{
  "name": "open-data-certificate-node",
  "version": "2.0.0",
  "description": "Modern Node.js implementation of the Open Data Certificate questionnaire system",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "debug": "DEBUG_LEVEL_CALC=true npm start",
    "migrate": "node scripts/migrateSurveys.js",
    "migrate:surveys:single": "SINGLE_MODE=true node scripts/migrateSurveys.js",
    "migrate:surveys:single:clear": "SINGLE_MODE=true CLEAR_MONGO=true node scripts/migrateSurveys.js",
    "migrate:surveys:full": "SINGLE_MODE=false node scripts/migrateSurveys.js",
    "migrate:surveys:full:clear": "SINGLE_MODE=false CLEAR_MONGO=true node scripts/migrateSurveys.js",
    "migrate:certificate": "node scripts/migrateCertificates.js",
    "migrate:certificates:single": "SINGLE_MODE=true node scripts/migrateCertificates.js",
    "migrate:certificates:single:clear": "SINGLE_MODE=true CLEAR_MONGO=true node scripts/migrateCertificates.js",
    "migrate:certificates:full": "SINGLE_MODE=false node scripts/migrateCertificates.js",
    "migrate:certificates:full:clear": "SINGLE_MODE=false CLEAR_MONGO=true node scripts/migrateCertificates.js",
    "test:env": "node scripts/test-env.js",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "open-data",
    "certificate",
    "questionnaire",
    "mongodb",
    "nodejs",
    "express"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/open-data-certificate-node.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/open-data-certificate-node/issues"
  },
  "homepage": "https://github.com/yourusername/open-data-certificate-node#readme"
}
